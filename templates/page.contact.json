{
  "sections": {
    "main": {
      "type": "main-contact",
      "disabled": true,
      "settings": {
        "show_contact_title": false,
        "heading_align": "text-center"
      }
    },
    "image-banner": {
      "type": "image-banner",
      "blocks": {
        "heading": {
          "type": "heading",
          "settings": {
            "heading": "Contact Us",
            "heading_size": "h1",
            "heading_h1": true
          }
        },
        "text": {
          "type": "text",
          "settings": {
            "text": "<p>Please visit our help pages for answers to some of the most commonly asked questions.<\/p>",
            "enlarge_text": true
          }
        }
      },
      "block_order": [
        "heading",
        "text"
      ],
      "settings": {
        "url": "",
        "height_mode": "fixed",
        "fixed_height_desktop": 560,
        "fixed_height_mobile": 440,
        "overlay_position": "justify-start items-center",
        "overlay_text_align": "text-start",
        "mob_center_text": true,
        "mobile_stacked": false,
        "color_scheme": "white",
        "full_width": true,
        "transparent_content_color": "#ffffff",
        "tint_color": "#000000",
        "tint_opacity": 40,
        "prevent_animation": false
      }
    },
    "form": {
      "type": "contact-form",
      "blocks": {
        "contact-name-2": {
          "type": "name",
          "settings": {
            "required": true,
            "half_width": true
          }
        },
        "contact-email-3": {
          "type": "email",
          "settings": {
            "required": true,
            "half_width": true
          }
        },
        "contact-dropdown-4": {
          "type": "dropdown",
          "settings": {
            "title": "How can we help?",
            "options": "Option 1\nOption 2\nOption 3",
            "half_width": false
          }
        },
        "contact-body-5": {
          "type": "body",
          "settings": {
            "required": true,
            "half_width": false
          }
        }
      },
      "block_order": [
        "contact-name-2",
        "contact-email-3",
        "contact-dropdown-4",
        "contact-body-5"
      ],
      "settings": {
        "heading": "Drop us an email",
        "heading_align": "text-center",
        "image_position": "top",
        "color_scheme": "none",
        "full_width": false,
        "button_style": "btn btn--primary"
      }
    },
    "newsletter": {
      "type": "newsletter",
      "blocks": {
        "newsletter-heading-0": {
          "type": "heading",
          "settings": {
            "heading": "Sign up to our Newsletter",
            "heading_size": "h5"
          }
        },
        "newsletter-text-1": {
          "type": "text",
          "settings": {
            "text": "<p>Be the first to know about new collections and exclusive offers.<\/p>",
            "enlarge_text": false
          }
        },
        "newsletter-form-2": {
          "type": "form",
          "settings": {
          }
        }
      },
      "block_order": [
        "newsletter-heading-0",
        "newsletter-text-1",
        "newsletter-form-2"
      ],
      "settings": {
        "overlay_position": "justify-center",
        "text_align": "text-center",
        "color_scheme": "default",
        "full_width": true,
        "transparent_content_color": "#ffffff",
        "tint_color": "#000000",
        "tint_opacity": 20
      }
    }
  },
  "order": [
    "main",
    "image-banner",
    "form",
    "newsletter"
  ]
}
